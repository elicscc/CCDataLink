{
  "name": "cc_data_link",
  "version": "1.0.0",
  "author": "cc",
  "license": "UNLICENSED",
  "description": "CC数据库连接工具",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env INDUSTRY=tob node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis",
    "build32": "cross-env INDUSTRY=tob node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis:ia32",
    "build:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.qq.com node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis",
    "build32:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.qq.com node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis:ia32",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:zip": "node .electron-vue/build.js && electron-builder --windows zip",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:executor-linux": "cross-env BUILD_TARGET=executor EXECUTOR=1 node .electron-vue/build.js",
    "build:executor-win-tob": "cross-env PACKAGE=mini EXECUTOR=2 INDUSTRY=tob webpack --display-modules --progress --colors --config .electron-vue/webpack.executor.config.js && node .electron-vue/encrypt.js",
    "build:executor-win-toc": "cross-env PACKAGE=mini EXECUTOR=2 INDUSTRY=toc webpack --display-modules --progress --colors --config .electron-vue/webpack.executor.config.js && node .electron-vue/encrypt.js",
    "dev": "cross-env INDUSTRY=tob NODE_ENV=development node .electron-vue/dev-runner.js",
    "dev:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.baidu.com NODE_ENV=development node .electron-vue/dev-runner.js",
    "dev:ToCW": "cross-env INDUSTRY=toc TOC_URL=http://localhost:9528 NODE_ENV=development node .electron-vue/dev-runner.js",
    "dev:executor": "cross-env NODE_ENV=development EXECUTOR=2 webpack --display-modules --progress --colors --config .electron-vue/webpack.executor.config.js && node --inspect dist/executor.js",
    "rebuild": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix app"
  },
  "build": {
    "productName": "CC数据库连接",
    "appId": "www.cc.cn",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "win": {
      "requestedExecutionLevel": "highestAvailable",
      "icon": "static/logo/buildIco.ico"
    },
    "nsis": {
      "allowElevation": false,
      "include": "static/customNsis/installer.nsh",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraResources": [
      {
        "from": "static/",
        "to": "static/"
      }
    ]
  },
  "dependencies": {
    "java": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@metrichor/jmespath-plus": "^0.3.1",
    "@riophae/vue-treeselect": "^0.4.0",
    "@types/cheerio": "0.22.23",
    "@types/crypto-js": "^4.0.1",
    "@types/dom-parser": "^0.1.1",
    "@types/express": "^4.17.8",
    "@types/java": "^0.9.1",
    "@types/js-md5": "^0.4.2",
    "@types/mathjs": "^6.0.11",
    "@types/multipipe": "3.0.0",
    "@types/ping": "^0.2.1",
    "@types/request": "^2.48.5",
    "@types/ssh2-sftp-client": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
