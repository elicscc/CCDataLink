{
  "name": "cc_data_link",
  "version": "1.0.3",
  "author": "cc",
  "license": "UNLICENSED",
  "description": "CC数据库管理工具",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.cc.com node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis",
    "build32:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.cc.com node .electron-vue/build.js && node .electron-vue/en_code.js && electron-builder --windows nsis:ia32",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:zip": "node .electron-vue/build.js && electron-builder --windows zip",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev:ToC": "cross-env INDUSTRY=toc TOC_URL=https://www.cc.com NODE_ENV=development node .electron-vue/dev-runner.js",
    "rebuild": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix app"
  },
  "build": {
    "productName": "CC数据库管理工具",
    "appId": "www.cc.cn",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "win": {
      "requestedExecutionLevel": "highestAvailable",
      "icon": "static/logo/buildIco.ico"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "allowElevation": false,
      "include": "static/customNsis/installer.nsh",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraResources": [
      {
        "from": "static/",
        "to": "static/"
      }
    ]
  },
  "dependencies": {
    "java": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@metrichor/jmespath-plus": "^0.3.1",
    "@types/cheerio": "0.22.23",
    "@types/crypto-js": "^4.0.1",
    "@types/dom-parser": "^0.1.1",
    "@types/express": "^4.17.8",
    "@types/java": "^0.9.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "cfonts": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^5.0.5",
    "crypto-js": "^4.0.0",
    "css-loader": "^0.28.4",
    "electron": "^10.4.7",
    "electron-builder": "^22.8.0",
    "electron-re": "^1.1.1",
    "electron-store": "^8.0.1",
    "element-ui": "^2.15.7",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-webpack-plugin": "^2.5.1",
    "gethue": "4.8.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "mini-css-extract-plugin": "^1.2.1",
    "monaco-editor": "0.29.1",
    "monaco-editor-webpack-plugin": "5.0.0",
    "multispinner": "^0.2.1",
    "request": "^2.88.2",
    "sass": "^1.26.10",
    "sass-loader": "^7.0.3",
    "sql-formatter": "2.3.3",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "^6.0.11",
    "terser-webpack-plugin": "^4.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^1.0.1",
    "vue": "^2.6.14",
    "vue-drag-select-pro": "^1.0.4",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.14",
    "vue-uuid": "^2.0.2",
    "vuex": "3.0.1",
    "vxe-table": "3.4.12",
    "xe-utils": "3.5.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
