{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": 10
            }
          }
        ]
      ]
    },
    "renderer": {
      "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "targets": {
              "chrome": "70"
            }
          }
        ]
      ]
    },
    "executor": {
      "presets": [
        "@babel/typescript"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "transform-vue-jsx"
  ]
}
